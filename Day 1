#include <stdio.h>

int main() {
    int n, pos, x;

    // Read size of array
    scanf("%d", &n);

    int arr[1000];  // sufficient buffer for typical constraints

    // Read array elements
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read position (1-based)
    scanf("%d", &pos);

    // Read element to insert
    scanf("%d", &x);

    // Convert 1-based position to 0-based index
    int index = pos - 1;

    // Shift elements to the right
    for(int i = n; i > index; i--) {
        arr[i] = arr[i - 1];
    }

    // Insert the element
    arr[index] = x;

    // Print updated array
    for(int i = 0; i < n + 1; i++) {
        printf("%d", arr[i]);
        if(i < n) printf(" ");
    }

    return 0;
}
